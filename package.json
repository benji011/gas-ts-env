{
  "name": "gas-es6-template",
  "description": "Google Apps Script ES6 env",
  "scripts": {
    "output": "browserify -t babelify -p gasify src/xxx.js -o dist/xxx.js",
    "setup": "npm i -g @google/clasp && npm run login",
    "login": "clasp login",
    "create": "clasp create",
    "clone": "clasp clone",
    "pull": "clasp pull",
    "push": "clasp push"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-preset-gas": "^2.0.4"
  }
}
