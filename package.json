{
  "name": "gas-ts-env",
  "description": "Environment to develop Google Apps Script with TypeScript",
  "scripts": {
    "login": "clasp login",
    "make": "clasp create",
    "clone": "clasp clone",
    "pull": "clasp pull",
    "dev": "babel src/ -d dist/ -w",
    "build": "babel src/ -d dist/ --minified",
    "push": "clasp push"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@google/clasp": "^2.3.0",
    "babel-preset-gas": "^2.0.4"
  }
}
